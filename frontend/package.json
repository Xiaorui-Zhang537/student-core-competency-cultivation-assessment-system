{
  "name": "student-assessment-platform",
  "version": "0.2.2",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n FRONTEND,DOCS \"vite\" \"npm --prefix ../docs run docs:dev\"",
    "dev:solo": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "frontend:dev": "npm --prefix frontend run dev",
    "docs:dev": "vitepress dev docs --port 4174 --host",
    "dev:all": "concurrently -n FRONTEND,DOCS \"npm:frontend:dev\" \"npm:docs:dev\""
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.2.0",
    "@vueuse/core": "^10.5.0",
    "@vueuse/motion": "^2.0.0",
    "axios": "^1.6.2",
    "echarts": "^5.4.3",
    "gsap": "^3.13.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "pinia": "^2.1.7",
    "three": "^0.179.1",
    "vue": "^3.5.18",
    "vue-i18n": "^9.10.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@tailwindcss/typography": "^0.5.10",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.19.11",
    "@types/three": "^0.179.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/compiler-sfc": "^3.5.18",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/language-core": "^3.0.4",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.4.0",
    "vite": "^5.0.0",
    "vitepress": "^1.6.4",
    "vue-tsc": "^3.0.4"
  }
}
